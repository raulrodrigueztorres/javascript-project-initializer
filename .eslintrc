{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": ["standard", "prettier"],
	"plugins": ["prettier"],
	"globals": {
		"Atomics": "readonly",
		"SharedArrayBuffer": "readonly"
	},
	"parserOptions": {
		"ecmaVersion": 12,
		"sourceType": "module"
	},
	"rules": {
		"semi": ["error", "always"],
		"eol-last": 0
	}
}
